<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button layoutX="188.0" layoutY="238.0" mnemonicParsing="false" onAction="#GoToAdmin" prefHeight="159.0" prefWidth="171.0" text="ADMIN">
         <graphic>
            <ImageView fx:id="admminIcon" fitHeight="114.0" fitWidth="110.0" pickOnBounds="true" preserveRatio="true" />
         </graphic></Button>
      <Button layoutX="442.0" layoutY="238.0" mnemonicParsing="false" onAction="#GoToPublic" prefHeight="159.0" prefWidth="171.0" text="PUBLIC" textAlignment="CENTER" wrapText="true">
         <graphic>
            <ImageView fx:id="pubIcon" fitHeight="101.0" fitWidth="107.0" pickOnBounds="true" preserveRatio="true" />
         </graphic></Button>
      <ImageView fx:id="imageview" fitHeight="224.0" fitWidth="300.0" layoutX="251.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <ToolBar layoutY="564.0" prefHeight="36.0" prefWidth="802.0">
         <items>
            <Button mnemonicParsing="false" onAction="#quitter" prefHeight="14.0" prefWidth="82.0" text="QUITTER" />
         </items>
      </ToolBar>
      <VBox alignment="TOP_CENTER" layoutX="321.0" layoutY="500.0" prefHeight="73.0" prefWidth="159.0">
         <children>
            <Label text="Développé par " textAlignment="CENTER" />
            <Hyperlink fx:id="link" text="Chems Eddine Benaziza" textAlignment="CENTER" />
            <Hyperlink text="Oussama Benakmoum" textAlignment="CENTER" />
         </children>
      </VBox>
   </children>
</AnchorPane>
